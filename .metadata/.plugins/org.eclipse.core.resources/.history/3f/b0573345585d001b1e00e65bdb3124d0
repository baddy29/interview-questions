package org.link.structure;

public class LinkedListRunner {

	static Node head=new Node();
	static ListOperations operation=new ListOperations();
	public static void main(String[] args) {
		operation.insert(10, head);
		operation.insert(20, head);
		operation.insert(30, head);
		operation.insert(40, head);
		operation.insert(50, head);
		System.out.println("List created");
		operation.show(head);		
		
		operation.insertAt(3, head, 110);
		System.out.println("110 inserted at 3rd position");
		
		operation.insertAtBegining(head, 120);
		System.out.println("120 inserted at begining");
		
		operation.insertAtEnd(head, 130);
		System.out.println("120 inserted at end");
		
		operation.deleteAt(3, head);
		System.out.println("Element deleted at 3rd position");

	}
}
