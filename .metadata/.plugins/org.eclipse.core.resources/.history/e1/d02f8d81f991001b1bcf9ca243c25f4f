package com.practice;

import java.util.Scanner;

public class Runner {
	public static void main(String[] args) {

		int[] array = new int[10];
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter nuts");
		for (int i = 0; i < 10; i++) {
			array[i] = sc.nextInt();
		}
		sc.close();
	}
}
