package org.pkg;

public class MainClass {
	public static void main(String[] args) {
		int ar[] = { -2, -3, 4, -1, -2, 1, 5, -3};
		int curr_max = 0;
		boolean reset = false;
		int max_so_far = Integer.MIN_VALUE;
		for (int i = 0; i < ar.length; i++) {
			curr_max = curr_max + ar[i];
			if (reset) {
				max_so_far = curr_max;
			}
			if (curr_max < 0) {
				reset=true;
			}

		}
		System.out.println(max_so_far);
	}
}
