package org.count.frequency;

public class BitManipulation {
	public static void main(String[] args) {
			
		int ar[]= {1,2,1,4,5,5,0,4,2};
		int x=ar[0]^ar[1];
		boolean isZeroPresent=false;
		int zeroCount;
		for(int i =2;i<ar.length;i++) {
			x=x^ar[i];
			if(ar[i]==0) {
				isZeroPresent=true;
			}
		}
		System.out.println(x);
	}
}
