package main;

public class BubbleSort {

	public static void main(String[] args) {
		int ar[] = { 7, 3, 4, 5, 6, 8, 2, 8 };
		int flag=0;
		for (int i = 0; i < ar.length - 1; i++) {
			flag=0;
			for (int j = 0; j < ar.length - 1 - i; j++) {
				if (ar[j] > ar[j + 1]) {
					int temp = ar[j];
					ar[j] = ar[j + 1];
					ar[j + 1] = temp;
					flag=1;
				}
				if(flag==0) {
					
				}
			}
		}
		
		for (int i = 0; i < ar.length; i++) {
			System.out.print(ar[i] + ", ");
		}
	}

}
